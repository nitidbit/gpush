{
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
      "options": {
        "semi": false,
        "singleQuote": true,
        "bracketSpacing": true
      }
    },
    {
      "files": ["*.rb", "*.arb", "*.rake"],
      "options": {
        "trailingComma": "all"
      }
    }
  ]
}
